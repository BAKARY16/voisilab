# Configuration npm pour VoisiLab Front-End

# Désactiver les avertissements de compatibilité React 19
legacy-peer-deps=true

# Ignore les warnings de dépendances
audit=false

# Utiliser les lockfiles existants
package-lock=true

# Niveau de log
loglevel=warn

# Sauvegarde automatique des dépendances
save-exact=false
