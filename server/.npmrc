# Configuration npm pour VoisiLab Server

# Activer le cache partagé
cache=.npm-cache

# Ignore les warnings de dépendances
audit=false

# Utiliser les lockfiles existants
package-lock=true

# Niveau de log
loglevel=warn

# Sauvegarde automatique des dépendances
save-exact=false

# Ignorer les scripts optionnels
ignore-scripts=false
